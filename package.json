{
    "name": "no-eval-handlebars",
    "description": "A custom version of the handlebars package which, to improve security, does not use 'eval' or 'new Function'",
    "version": "1.0.2",
    "private": false,
    "license": "MIT",
    "author": "",
    "keywords": [
        "@elastic/handlebars",
        "kbn/handlebars",
        "no-eval-handlebars",
        "kbn-handlebars",
        "handlebars no eval"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rm -rf ./lib",
        "test": "jest",
        "watch": "tsc -w"
    },
    "dependencies": {
        "handlebars": "4.7.8"
    },
    "devDependencies": {
        "@babel/core": "^7.24.7",
        "@babel/preset-env": "^7.24.7",
        "@babel/preset-typescript": "^7.24.7",
        "@types/jest": "^29.5.3",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "tslib": "^2.6.3",
        "typescript": "^4.5.5"
    }
}
